  },
  "properties": {
    "codigo": {
      "type": "string",
      "id": true,
      "required": true
    },
    "nome": {
      "type": "string",
      "required": true
    },
    "website": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "_id": {
      "type": "number",
      "generated": true
    }
  },
  "validations": [],
  "relations": {
    "departamento": {
      "type": "belongsTo",
      "model": "Departamento",

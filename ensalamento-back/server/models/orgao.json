{
  "name": "Orgao",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nome": {
      "type": "string",
      "required": true
    },
    "localizacao": {
      "type": "geopoint"
    },
    "codigo": {
      "type": "string",
      "id": true,
      "required": true
    },
    "_id": {
      "type": "number",
      "generated": true
    }
  },
  "validations": [],
  "relations": {
    "secretario": {
      "type": "belongsTo",
      "model": "Secretario",
      "foreignKey": "secretarioId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "*"
    },
    {
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "*"
    }
  ],
  "methods": {}
}
